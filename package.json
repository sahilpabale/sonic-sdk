{
  "name": "@0xsonic/sdk",
  "version": "0.1.6",
  "description": "Sonic SDK enables you to quickly setup and integrate your decentralized community in a flash!",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && npm run prettier && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "prepublishOnly": "npm run build",
    "localPub": "npm run build && npm pack"
  },
  "repository": "https://github.com/sahilpabale/sonic-sdk.git",
  "author": "Sahil Pabale <dev.sahilpabale@gmail.com> (https://sahil.lol)",
  "contributors": [
    "Anish De <contact@anishde.dev> (https://github.com/AnishDe12020)"
  ],
  "keywords": [
    "react",
    "sonic",
    "decentralized",
    "community-building",
    "orbis"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@orbisclub/orbis-sdk": "^0.4.4-beta.41",
    "framer-motion": "^7.10.0",
    "recoil": "^0.7.6"
  }
}
